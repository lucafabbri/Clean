<Project>

    <PropertyGroup>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageOutputPath>nupkgs</PackageOutputPath>
        <LangVersion>latestmajor</LangVersion>
        <RepositoryUrl>https://github.com/lucafabbri/Clean</RepositoryUrl>
        <Authors>Luca Fabbri</Authors>
        <RepositoryType>git</RepositoryType>
        <Copyright>2024 Luca Fabbri</Copyright>
        <AssemblyVersion>$(Version)</AssemblyVersion>
        <FileVersion>$(Version)</FileVersion>
        <PackageTags>Clean</PackageTags>
        <PackageProjectUrl>https://github.com/lucafabbri/Clean</PackageProjectUrl>
        <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <DocumentationMarkdown>$(MSBuildProjectDirectory)\README.md</DocumentationMarkdown>
    </PropertyGroup>

</Project>